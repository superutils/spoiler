        "@storybook/testing-library": "0.0.9",
        "@types/react": "^16.9.55",
        "@types/react-dom": "^16.9.9",
        "@types/styled-components": "^5.1.4",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "babel-loader": "^8.2.4",
        "concurrently": "^5.3.0",
        "install-peers-cli": "^2.2.0",
        "prepend-file": "^2.0.0",
