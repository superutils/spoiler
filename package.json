{
  "name": "@ungarson/spoiler",
  "version": "0.10.0",
  "description": "React component to hide other components, inspired by spoilers in telegram",
  "author": "@superutils",
  "keywords": [
    "superutils",
    "spoiler",
    "telegram",
    "hide",
    "message"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superutils/spoiler.git"
  },
  "private": false,
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@types/node": "^16.11.39",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "ts-standard": "^11.0.0",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "start": "PORT=3124 react-scripts start",
    "build": "npm run build:ts && npm run build:cjs && cp ./src/assets ./lib/esm && cp ./src/assets ./lib/cjs",
    "build:ts": "tsc --module es2015 --outDir lib/esm",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2"
  },
  "files": [
    "/lib/esm/",
    "/lib/cjs/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts"
}
